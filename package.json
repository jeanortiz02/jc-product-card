    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
