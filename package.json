{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanortiz02/jc-product-card"
  },
  "homepage": "https://jeancarlosortiz.com/",
  "keywords": [
    "react",
